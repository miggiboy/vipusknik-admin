{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "cviebrock/eloquent-sluggable": "^4.2",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelrus/localized-carbon": "dev-master",
        "sentry/sentry-laravel": "^0.6.1",
        "spatie/laravel-medialibrary": "^5.11",
        "yzalis/identicon": "^1.1",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "/usr/local/php-cgi/7.1/bin/php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "/usr/local/php-cgi/7.1/bin/php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "/usr/local/php-cgi/7.1/bin/php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "/usr/local/php-cgi/7.1/bin/php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
